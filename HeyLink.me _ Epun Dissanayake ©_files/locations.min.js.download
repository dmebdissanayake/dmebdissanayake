function initMap(id,locations,lock){if(typeof locations==='undefined'){return;}
let is_lock=JSON.parse(lock.toLowerCase());let markerLocations=JSON.parse(locations);let gestureHandling="auto";if(is_lock){gestureHandling="none";}
const firstMarkerPositionCenter={lat:Number(markerLocations[0].lat),lng:Number(markerLocations[0].lng),};const map=new google.maps.Map(document.getElementById(id),{zoom:10,center:firstMarkerPositionCenter,mapTypeControl:false,disableDefaultUI:is_lock,zoomControl:!is_lock,gestureHandling:gestureHandling,streetViewControl:false,});markerLocations.map((item)=>{return new google.maps.Marker({position:{lat:parseFloat(item.lat),lng:parseFloat(item.lng)},map:map,});});}
$(document).ready(function(){let locationLink=$('.map-title-wrapper');locationLink.click(function(){const locationID=$(this).data('id');$('.map-info-wrapper[data-id='+locationID+']').toggleClass('hidden');$('.location-up[data-id='+locationID+']').toggleClass('hidden');})
$('.location-btn-up').click(function(){const locationID=$(this).data('id');$('.map-info-wrapper[data-id='+locationID+']').toggleClass('hidden');$('.location-up[data-id='+locationID+']').addClass('hidden');})})